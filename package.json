{
	"name": "kirim",
	"displayName": "Kirim",
	"description": "Send test HTML emails within Visual Studio Code",
	"version": "0.0.4",
	"publisher": "thomasaustin",
	"author": {
		"name": "Thomas Austin"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thomasaustin/kirim"
	},
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Email",
		"HTML",
		"Send",
		"Test"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#1c252d",
		"theme": "dark"
	},
	"license": "MIT",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.loadconfig",
				"title": "Kirim: Load Config"
			},
			{
				"command": "extension.saveconfig",
				"title": "Kirim: Save Config"
			},
			{
				"command": "extension.clearconfig",
				"title": "Kirim: Clear Config"
			},
			{
				"command": "extension.deleteconfig",
				"title": "Kirim: Delete Config"
			},
			{
				"command": "extension.sendemail",
				"title": "Kirim: Send Email"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^12.7.5",
		"tslint": "^5.20.0",
		"typescript": "^3.6.3",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"@sendgrid/mail": "^6.4.0",
		"front-matter": "^3.0.2"
	}
}
